# ── Tempo Testnet ────────────────────────────────
# Deployer / backend signer private key (with 0x prefix)
AGENT_PRIVATE_KEY=0xYOUR_PRIVATE_KEY_HERE

# Agent payment receiver address
AGENT_RECEIVER_ADDRESS=0xYOUR_RECEIVER_ADDRESS

# ── AI (at least one recommended for real AI) ────
# Groq — FREE, no credit card → https://console.groq.com/keys
GROQ_API_KEY=gsk_YOUR_KEY_HERE
# OpenAI — paid fallback
OPENAI_API_KEY=sk-YOUR_KEY_HERE

# ── Privy (optional — app works without it) ──────
NEXT_PUBLIC_PRIVY_APP_ID=YOUR_PRIVY_APP_ID

# ── Demo Mode ────────────────────────────────────
NEXT_PUBLIC_DEMO_MODE=true
